<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainPageActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textShortMessage"
            android:text="Search"
            android:ems="10"
            tools:layout_editor_absoluteY="19dp" tools:layout_editor_absoluteX="26dp" android:id="@+id/editText2"
            tools:text="Search" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="66dp"
            android:layout_height="46dp"
            android:inputType="none"
            android:text="Title:"
            android:ems="10"
            tools:layout_editor_absoluteY="84dp" android:id="@+id/editText3"
            style="@android:style/Widget.AutoCompleteTextView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.075"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="66dp"
            android:layout_height="46dp"
            android:inputType="none"
            android:text="Year:"
            android:ems="10"
            tools:layout_editor_absoluteY="153dp" android:id="@+id/editText4"
            style="@android:style/Widget.AutoCompleteTextView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.075"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="66dp"
            android:layout_height="46dp"
            android:inputType="none"
            android:text="Star:"
            android:ems="10"
            tools:layout_editor_absoluteY="293dp" android:id="@+id/editText6"
            style="@android:style/Widget.AutoCompleteTextView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.075"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="86dp"
            android:layout_height="46dp"
            android:inputType="none"
            android:text="Directer:"
            android:ems="10"
            tools:layout_editor_absoluteY="221dp" android:id="@+id/editText5"
            style="@android:style/Widget.AutoCompleteTextView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.08"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:text="TextView"
            android:layout_width="121dp"
            android:layout_height="36dp" tools:layout_editor_absoluteY="94dp"
            android:id="@+id/titleView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editText3" app:layout_constraintHorizontal_bias="0.267"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:text="TextView"
            android:layout_width="121dp"
            android:layout_height="36dp" tools:layout_editor_absoluteY="163dp"
            android:id="@+id/yearView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editText4" app:layout_constraintHorizontal_bias="0.267"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:text="TextView"
            android:layout_width="121dp"
            android:layout_height="36dp" tools:layout_editor_absoluteY="231dp"
            android:id="@+id/directerView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editText5" app:layout_constraintHorizontal_bias="0.185"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:text="TextView"
            android:layout_width="121dp"
            android:layout_height="36dp" tools:layout_editor_absoluteY="303dp"
            android:id="@+id/starView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editText6" app:layout_constraintHorizontal_bias="0.267"
            tools:ignore="MissingConstraints"/>
    <Button
            android:text="Search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="389dp"
            tools:layout_editor_absoluteX="30dp" android:id="@+id/searchButton" tools:ignore="MissingConstraints"/>
</android.support.constraint.ConstraintLayout>